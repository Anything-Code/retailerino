datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

generator nexusPrisma {
  provider = "nexus-prisma"
}

/// A user
model User {
  id              Int    @id @default(autoincrement())
  name            String
  email           String
  password        String
}